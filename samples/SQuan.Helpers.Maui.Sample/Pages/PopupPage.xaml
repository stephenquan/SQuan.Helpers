<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="SQuan.Helpers.Maui.Sample.PopupPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="PopupPage">
    <VerticalStackLayout Padding="25,25,25,25" Spacing="25">
        <Button Clicked="OnIntPopup" Text="Open Integer Popup" />
        <Button Clicked="OnDblPopup" Text="Open Double Popup" />
    </VerticalStackLayout>
</ContentPage>
